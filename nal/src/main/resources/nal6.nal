            (S ==> M), (--M ==> P), task("?") |- ((S ==>+- P) || (P ==>+- S)), (Punctuation:Question)
                //(S ==> M), (--M ==> P), neqRCom(S,P), task("?") |- (S ==>+- P), (Punctuation:Question)
