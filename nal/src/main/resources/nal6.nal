        (S ==> M), (M ==> P), neqRCom(S,P), task("?") |- (P ==>+- S), (Punctuation:Question)

        //if after S --M happens, and after --M then P happens, so S then P happens
        (S ==> M), (--M ==> P) |- (S ==>+- P), (Belief:DeductionN)
            (S ==> M), (--M ==> P), neqRCom(S,P), task("?") |- (S ==>+- P), (Punctuation:Question)
            (S ==> M), (--M ==> P), neqRCom(S,P), task("?") |- (P ==>+- S), (Punctuation:Question)

