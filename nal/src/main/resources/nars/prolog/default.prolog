 /* AND */
'&&'(A, B) :- A , B.
'&&'(A, B, C) :- A , B, C.

/* OR */
'||'(A, B) :- A ; B.
'||'(A, B, C) :- A ; B ; C.


'-->'(A, C) :- not(A=C), '-->'(A,B), '-->'(B,C).
'==>'(A, C) :- not(A=C), '==>'(A,B), '==>'(B,C).

'<->'(A, B) :- not(A=B), '-->'(A,B), '-->'(B,A).
'<->'(A, B) :- not(A=B), '-->'(B,A).



