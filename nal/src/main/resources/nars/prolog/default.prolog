
'-->'(A, C) :- A\=C, '-->'(A,B), '-->'(B,C).
'==>'(A, C) :- A\=C, '==>'(A,B), '==>'(B,C).

'<->'(A, B) :- A\=B, '-->'(A,B), '-->'(B,A).
'<->'(A, B) :- A\=B, '-->'(B,A).

'--'('<->'(A, B)) :- A\=B, '--'('-->'(A,B)), '-->'(B,A).
A :- '--'('--'(A)).


