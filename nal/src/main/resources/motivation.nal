    (X-->Y), X, task("!") |-          (?1 ==>+- (X-->Y)), (Punctuation:Question)
    (X-->Y), X, task("!") |-          ((X-->Y) ==>+- ?1), (Punctuation:Question)
    (X-->Y), X, task("!") |-        (--(X-->Y) ==>+- ?1), (Punctuation:Question)
    (X-->Y), X, task("!") |-         (?1 &&+- --(X-->Y)), (Punctuation:Question)
    
    
